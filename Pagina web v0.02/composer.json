{
    "name": "Nicolaysito/Hobbyonthego",
    "description": "Eventos",
    "type": "application",
    "keywords": [
        "php",
        "webapp",
        "testing"
    ],
    "homepage": "https://github.com/Nicolaysito/Hobbyonthego",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicolay Joya",
            "email": "Nicolay.JG@outlook.com"            
        }
    ],
    "support": {
        "issues": "https://github.com/Nicolaysito/Hobbyonthego/issues"
    },
    "require" : {
        "php" : ">=5.6",
        "ext-pdo_sqlite" : "*",
        "symfony/console" : "~3.2.1@stable",
        "phpunit/phpunit" : "~5.7.3@stable",
        "silex/silex" : "~2.0@stable",
        "jdesrosiers/silex-cors-provider" : "~1.0@stable",
        "symfony/finder" : "~3.1@stable",
        "doctrine/dbal" : "~2.5@stable"
    },
    "minimum-stability" : "stable",
    "support" : {
        "source" : "https://github.com/Nicolaysito/Hobbyonthego/tree/master/Pagina%20web%20v0.02",
        "issues" : "https://github.com/Nicolaysito/Hobbyonthego/issues"
    },
    "config" : {
        "bin-dir" : "bin/",
        "vendor-dir" : "vendor/"
    },
    "autoload" : {
        "psr-4" : {
            "" : "tests/_files"
        }
    },
    "require-dev" : {
        "ext-mbstring" : "*"
    },
    "suggest" : {
        "phpunit/dbunit" : "The DbUnit extension considerably simplifies the setup of a database for testing"
    }
}